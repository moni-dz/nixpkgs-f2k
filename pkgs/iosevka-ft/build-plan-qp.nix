''
  [buildPlans.iosevka-ft-qp]
  family = "Iosevka FT QP"
  spacing = "quasi-proportional"
  serifs = "slab"
  no-cv-ss = false
  export-glyph-names = true

    [buildPlans.iosevka-ft-qp.variants]
    inherits = "ss03"

      [buildPlans.iosevka-ft-qp.variants.design]
      g = "single-storey-serifed"
      t = "flat-hook"
      y = "cursive-motion-serifed"
      lower-lambda = "curly-turn"
      cyrl-capital-u = "cursive"
      zero = "dotted"
      one = "base-long-top-serif"
      two = "curly-neck"
      three = "flat-top"
      four = "closed"
      five = "oblique-upper-left-bar"
      seven = "curly-serifed"
      underscore = "above-baseline"
      paragraph-sign = "low"
      brace = "curly-flat-boundary"
      ampersand = "upper-open"
      percent = "rings-continuous-slash"
      lig-ltgteq = "slanted"
      ascii-single-quote = "raised-comma"
      ascii-grave = "raised-turn-comma"
      question = "corner"
      punctuation-dot = "square"

      [buildPlans.iosevka-ft-qp.variants.italic]
      f = "diagonal-tailed"
      g = "single-storey-serifless"
      x = "curly-serifed"

      [buildPlans.iosevka-ft-qp.variants.oblique]
      f = "diagonal-tailed"
      g = "single-storey-serifed"

    [buildPlans.iosevka-ft-qp.ligations]
    inherits = "dlig"
''
